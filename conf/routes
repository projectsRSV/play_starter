


GET         /                           controllers.HomeController.index()
GET         /products                   controllers.HomeController.list()
GET         /products/new               controllers.HomeController.showCreateForm()
POST        /products                   controllers.HomeController.save()
GET         /products/:id               controllers.HomeController.showEditForm(id:Long)
POST        /products/:id               controllers.HomeController.update(id:Long)
POST        /products/:id/delete        controllers.HomeController.delete(id:Long)


GET           /assets/*file              controllers.Assets.versioned(path="/public", file: Asset)

